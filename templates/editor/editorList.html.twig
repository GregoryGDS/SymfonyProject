{% extends 'base.html.twig' %}

{% block title %}Editor list{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Liste d'étiteur</h1>
    <ul>
        {# légèrement différent de php avec foreach editorList as editor #}
        {% for editor in editorList %}
            <li>
               Nom d'entreprise : {{ editor.companyName }} <br>
               Nationalité : {{ editor.nationality }} <br>
            </li>
        {% endfor %}
    </ul>
</div>
{% endblock %}
